<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Ause Sims's Website</title>

    <environment include="Development">
        <link href="~/lib/bootstrap/dist/4.3.1/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <link href="~/css/custom-format.css" rel="stylesheet" />
    </environment>

    <environment exclude="Development">
        <link href="~/lib/bootstrap/dist/4.3.1/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
        <link href="~/css/custom-format.css" rel="stylesheet" />
    </environment>

</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-dark bg-dark fixed-top">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">AUSE SIMS</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerPrimary"
                aria-controls="navbarTogglerPrimary" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar active" id="navbarTogglerPrimary">

        </div>
    </nav>

    <nav class="navbar navbar-expand-md navbar-dark" style="background-color:rgba(49, 67, 179, 0.94)">
        <a class="navbar-brand" href="#"></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerSecondary"
                aria-controls="navbarTogglerSecondary" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerSecondary">
            <ul class="navbar-nav">
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">HOME <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Projects">PROJECTS <span class="sr-only">(current></span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Portfolio">PORTFOLIO <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Sites">SITES <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Blog">BLOG <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Art">ART <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">ABOUT <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">CONTACT <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">PRIVACY <span class="sr-only">(current)</span></a></li>
                <li class="nav-item active"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="ChangeLog">CHANGE LOG <span class="sr-only">(current)</span></a></li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid">
        <br />
        @RenderBody()
        <hr />
    </div>

    @*
    <div class="navbar fixed-bottom">
        <partial name="\_PartialContent\_Policies\_CookieConsentPartial" />
    </div>

    *@

    <footer>
        <p>Site Created by (Ause L. Sims) Copyright &copy; 2015 - @DateTime.Now.Year - All rights reserved.</p>
    </footer>


    <script src="~/lib/jQuery/dist/jquery-3.4.1.js.js"></script>
    <script src="~/lib/bootstrap/dist/4.3.1/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*
        The following code are Facebook features and functions to be implimented.
        This code is not to go into production until a Privacy Policy for the
        Facebook App on developer.facebook.com has been established and approved.

        <div id="fb-root"></div>
        <script async defer crossorigin="anonymous" src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v4.0&appId=612092265647135&autoLogAppEvents=1"></script>

    *@


    @RenderSection("Scripts", required: false)
</body>
</html>
